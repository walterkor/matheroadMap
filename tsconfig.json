{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "esnext",
    "module": "esnext",
    "allowJs": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "lib": ["dom", "esnext"],
    "types": [
      "nuxt",
      "vue",
      "node",
      "@types/google.maps",
      "@types/node",
      "@nuxt/types"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "#app": ["./.nuxt/types/app"]
    }
  }
}
